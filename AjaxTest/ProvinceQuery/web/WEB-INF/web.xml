<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>QueryProvinceByIdServlet</servlet-name>
        <servlet-class>com.province.controller.QueryProvinceByIdServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FuzzySearchServlet</servlet-name>
        <servlet-class>com.province.controller.FuzzySearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CityCountServlet</servlet-name>
        <servlet-class>com.province.controller.CityCountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CityCountServlet</servlet-name>
        <url-pattern>/citycount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FuzzySearchServlet</servlet-name>
        <url-pattern>/fuzzysearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryProvinceByIdServlet</servlet-name>
        <url-pattern>/queryprovincebyid</url-pattern>
    </servlet-mapping>
</web-app>